{
	"manifest_version": 2, 

	"name": "Getting started example",
	"description": "This extension shows a Google Image search result for the current page",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	]


}

// manifest_version 1 is deprecated
// browser action --> default icon == the icon users will see next to the omnibar
// browser action --> default popup == the popup that will appear when icon is clicked
// permission --> the resources you are requesting access to